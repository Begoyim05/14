#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;

    vector<vector<int>> A(n, vector<int>(n, 0));

    for (int i = 0; i < m; ++i) {
        int u, v;
        cin >> u >> v;
        if (u == v) continue; 

        A[u][v] = 1;
        A[v][u] = 1;
    }

    int q;
    cin >> q;

    while (q--) {
        string type;
        cin >> type;

        if (type == "ADJ") {
            int u, v;
            cin >> u >> v;

            if (A[u][v] == 1)
                cout << "YES\n";
            else
                cout << "NO\n";
        }

        else if (type == "DEG") {
            int v;
            cin >> v;

            int degree = 0;
            for (int j = 0; j < n; j++)
                degree += A[v][j];

            cout << degree << "\n";
        }
    }

    return 0;
}
